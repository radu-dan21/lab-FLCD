# P2, gcd of 2 numbers

int a = read("Introduceti primul numar: ");
int b = read("Introduceti cel de al doilea numar: ");

if (b > a) {
 a = a + b;
 b = a - b;
 a = a - b;
}

int aux;

while (b =/= 0) {
 aux = b;
 b = a mod b;
 a = aux;
}

print(@"Cel mai mare divizor comun este: {a}");
