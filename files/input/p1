# P1, max and min of 3 numbers

array[int, 3] v;
int i = 0;

while (i < 3) {
 v[i] = read(@"Introduceti numarul {i}: ");
 i = i + 1;
}

int maxim = v[0];
int minim = v[0];

i = 1;
while (i < 3) {
    if (v[i] > maxim) {
        maxim = v[i];
    }
    if (v[i] < minim) {
        minim = v[i];
    }
}

print(@"Maximul este: {maxim}");
print(@"Minimul else: {minim}");
